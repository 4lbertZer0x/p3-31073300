{
  "version": 2,
  "builds": [
    {
      "src": "app-vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app-vercel.js"
    }
  ]
  ,
  "env": {
    "NODE_ENV": "production"
  }
}
